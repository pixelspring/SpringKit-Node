    "express-session": "^1.15.6",
    "express-status-monitor": "^1.0.1",
    "express-validator": "^4.3.0",
    "knex": "^0.19.5",
    "lusca": "^1.5.2",
    "method-override": "^2.3.10",
    "moment": "^2.19.1",
