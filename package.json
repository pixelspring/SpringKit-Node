    "express-flash": "0.0.2",
    "express-session": "^1.15.2",
    "express-status-monitor": "^1.0.0",
    "express-validator": "^3.1.2",
    "knex": "^0.12.9",
    "lusca": "^1.4.1",
    "method-override": "^2.3.8",
    "moment": "^2.18.1",
    "morgan": "^1.8.1",
